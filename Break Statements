let arr = [1,2,3,4,5];
        for(let i in arr){
            document.write(arr[i],"<br>");
        }

        for(let i = 1;i<=8;i=i+2){
            if(i==4){
                break;
            }
            console.log(i);
        }

        for(let i = 1;i<=8;i=i+2){
            if(i>=4){
                break;
            }
            console.log(i);
        }
