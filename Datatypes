<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
        // let a = 1;
        // console.log("hi +a+a");
        // document.write('hi +a+a',"<br>");
        // document.write(`hi +${a+a}`)
        
        let x = "sai";
        let y = "kumar";
        let age = 30;
        let c = `I am ${x} ${y},my age is ${age} years old`;
        document.write(c);
        
    </script>
</body>
</html>
